SELECT
    site_short,
    region_short,
    contact_id,
    COUNT(DISTINCT college_app_id) AS applied_sum, 
    
FROM
    (SELECT *
    FROM `data-studio-260217.college_applications.college_application_filtered_table` 
    WHERE application_status = 'Applied')
AS applied_group

GROUP BY
    site_short,
    region_short,
    contact_id