WITH aggregate_data AS
(
SELECT
    site_short,
    region_short,
    Gender_c,
    first_generation_fy_20_c,
    indicator_low_income_c,
    Ethnic_background_c,
    COUNT(contact_id) AS student_count,
    
FROM `data-studio-260217.college_applications.college_application_filtered_table`
GROUP BY
    site_short,
    region_short,
    Gender_c,
    first_generation_fy_20_c,
    indicator_low_income_c,
    Ethnic_background_c
),

college_application_counts AS
(
SELECT
    site_short,
    COUNT(college_app_id) AS college_app_count,

FROM `data-studio-260217.college_applications.college_application_filtered_table` 

GROUP BY
    site_short,
    accepted

),

acceptance_counts AS
(
SELECT 
    accepted_sub.site_short, accepted_sum
    FROM
    (select site_short,college_app_id,SUM(accepted) AS accepted_sum
    FROM `data-studio-260217.college_applications.college_application_filtered_table` 
    WHERE application_status = "Applied"
    GROUP BY site_short,college_app_id) AS accepted_sub
    
),

college_aspiration_counts AS 
(
SELECT
    site_short,
    COUNT(aspiration_id) AS college_aspiration_count
FROM `data-studio-260217.college_applications.college_application_filtered_table`

GROUP BY site_short
)

SELECT 
    A.*,
    college_app_count,
    accepted_sum
    

FROM aggregate_data AS A
LEFT JOIN college_application_counts AS CA
    ON A.site_short = CA.site_short
LEFT JOIN acceptance_counts as accept     
    ON A.site_short = accept.site_short
LEFT JOIN college_aspiration_counts AS CAP
    ON A.site_short = CAP.site_short