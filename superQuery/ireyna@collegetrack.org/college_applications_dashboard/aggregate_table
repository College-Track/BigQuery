SELECT
    site_short,
    region_short,
    contact_id,
    COUNT(DISTINCT college_app_id) AS accepted_app_sum,
    COUNT(DISTINCT contact_id) AS accepted_student_sum
    
FROM
    (SELECT *
    FROM `data-studio-260217.college_applications.college_application_filtered_table` 
    WHERE accepted =1)
AS accepted_group

GROUP BY
    site_short,
    region_short,
    contact_id