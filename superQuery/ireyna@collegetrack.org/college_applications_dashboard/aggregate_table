WITH aggregate_data AS #contact demo data
(
SELECT
    site_short,
    region_short,
    Gender_c,
    first_generation_fy_20_c,
    indicator_low_income_c,
    Ethnic_background_c,
    COUNT(DISTINCT contact_id) AS student_count
    
FROM
    (SELECT site_short, region_short,contact_id, Gender_c, first_generation_fy_20_c, indicator_low_income_c, Ethnic_background_c,
    COUNT(DISTINCT contact_id)
    FROM `data-studio-260217.college_applications.college_application_filtered_table` 
    GROUP BY site_short,region_short, contact_id, Gender_c, first_generation_fy_20_c, indicator_low_income_c,
    Ethnic_background_c)
AS student_count
GROUP BY
    site_short,
    region_short,
    Gender_c,
    first_generation_fy_20_c,
    indicator_low_income_c,
    Ethnic_background_c
),

college_application_counts AS #count distinct college application ids
(
SELECT
    site_short,
    region_short,
    Gender_c,
    first_generation_fy_20_c,
    indicator_low_income_c,
    Ethnic_background_c,
    COUNT(DISTINCT college_app_id) AS college_app_count

FROM `data-studio-260217.college_applications.college_application_filtered_table` 

GROUP BY
    site_short,
    region_short,
    Gender_c,
    first_generation_fy_20_c,
    indicator_low_income_c,
    Ethnic_background_c
),

acceptance_counts AS #sum of acceptances across distinct college app ids 
(
SELECT
    site_short,
    region_short,
    Gender_c,
    first_generation_fy_20_c,
    indicator_low_income_c,
    Ethnic_background_c,
    COUNT(DISTINCT college_app_id) AS accepted_sum
    
FROM
    (SELECT *
    FROM `data-studio-260217.college_applications.college_application_filtered_table` 
    WHERE accepted =1)
AS accepted_group

GROUP BY
    site_short,
    region_short,
    Gender_c,
    first_generation_fy_20_c,
    indicator_low_income_c,
    Ethnic_background_c
),

college_aspiration_counts AS 
(
SELECT
    COUNT(DISTINCT aspiration_id) AS college_aspiration_count,
    site_short,
    region_short,
    Gender_c,
    first_generation_fy_20_c,
    indicator_low_income_c,
    Ethnic_background_c
FROM `data-studio-260217.college_applications.college_application_filtered_table`

GROUP BY 
    site_short,
    region_short,
    Gender_c,
    first_generation_fy_20_c,
    indicator_low_income_c,
    Ethnic_background_c
)

SELECT 
    A.*,
    college_app_count,
    college_aspiration_count,
    accepted_sum,
    
FROM aggregate_data AS A
LEFT JOIN college_application_counts AS CA   
    ON A.site_short = CA.site_short
LEFT JOIN acceptance_counts AS ACCPT
    ON A.site_short = ACCPT.site_short
LEFT JOIN college_aspiration_counts AS CAP
    ON A.site_short = CAP.site_short
    
GROUP BY
    site_short,
    region_short,
    Gender_c,
    first_generation_fy_20_c,
    indicator_low_income_c,
    Ethnic_background_c,
    student_count,
    college_app_count,
    college_aspiration_count,
    accepted_sum